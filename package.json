{
  "name": "graphql-cli-codegen",
  "version": "0.1.0",
  "description": "GraphQL CLI Apollo Codegen plugin",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint {src,test}/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rricard/graphql-cli-codegen.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rricard/graphql-cli-codegen/issues"
  },
  "homepage": "https://github.com/rricard/graphql-cli-codegen#readme",
  "devDependencies": {
    "@types/graphql": "^0.10.0",
    "@types/jest": "^20.0.5",
    "@types/node": "^8.0.17",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.7",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.2.0"
  },
  "dependencies": {
    "apollo-codegen": "^0.16.3",
    "graphql-cli": "^1.0.0-beta.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
